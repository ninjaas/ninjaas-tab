{
   "background": {
      "persistent": false,
      "scripts": [ "ninjaas.js" ]
   },
   "chrome_url_overrides": {
      "newtab": "dashboard.html"
   },
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "description": "Focus on just the things have to do",
   "icons": {
      "128": "icon-128.png",
      "16": "icon-16.png",
      "48": "icon-48.png"
   },
   "manifest_version": 2,
   "name": "Ninjaas",
   "offline_enabled": true,
   "permissions": [ "https://*.flickr.com/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.1"
}
